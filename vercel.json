{
  "version": 2,
   "builds": [
      {
          "src": "*.py",
          "use": "@vercel/python"
      }, 
      {
          "src": "./app.py",
          "use": "@vercel/python"
      }, 
     {
            "src": "./static/images",
            "use": "@vercel/static"
        }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "/"
      }
  
  
  ]
}
